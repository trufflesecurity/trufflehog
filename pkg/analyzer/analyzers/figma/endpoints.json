{
    "files:read": {
        "url": "https://api.figma.com/v1/me",
        "method": "GET",
        "expected_response_with_scope": {
            "status": 200
        },
        "expected_response_without_scope": {
            "status": 403,
            "message": "^Invalid scope(?:\\(s\\))?: ([a-zA-Z_:, ]+)\\. This endpoint requires the file_read or files:read or me scope\\.?"
        }
    },
    "library_analytics:read": {
